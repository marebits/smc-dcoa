{
	"name": "marecoin-dcoa",
	"version": "1.0.0",
	"description": "A Digital Certificate of Authenticity token for the Silver Mare Coin being minted by /mlp/ in 2022",
	"main": "",
	"scripts": {
		"build": "bin/build",
		"compile": "npx truffle compile",
		"console-ganache": "export NODE_OPTIONS=--openssl-legacy-provider && npx truffle console --network ganache",
		"dashboard": "npx truffle dashboard",
		"deploy": "npx truffle migrate --network development --reset",
		"deploy-dashboard": "npx truffle migrate --network dashboard --reset",
		"deploy-ganache": "npx truffle migrate --network ganache --reset",
		"generate-claim-codes": "npx truffle exec scripts/generateClaimCodes.js --network polygon",
		"github": "git commit -a && git push origin master",
		"svg": "npx svgo -i dcoa.svg -o dcoa.min.svg -p 1 --multipass"
	},
	"keywords": [
		"/mlp/",
		"marecoin",
		"coin",
		"certificate of authenticity",
		"nft",
		"erc721"
	],
	"author": "Twifag",
	"license": "LicenseRef-DSPL AND LicenseRef-NIGGER",
	"devDependencies": {
		"@opengsn/contracts": "^2.2.6",
		"@openzeppelin/contracts": "^4.5.0",
		"@truffle/hdwallet-provider": "^2.0.4",
		"svgo": "^2.8.0",
		"truffle": "^5.5.6"
	}
}
